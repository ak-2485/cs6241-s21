   "source": [
    "slow_loocv = zeros(dmax)\n",
    "for k = 1:dmax\n",
    "    for i = 1:npts\n",
    "        mask = BitArray(i != j for j = 1:npts)\n",
    "        cik = A[mask, 1:k]\\b[mask]\n",
    "        slow_loocv[k] += (b[i] - dot(A[i,1:k], cik))^2/npts\n",
    "    end\n",
    "end\n",
    "scatter(ks, slow_loocv, yscale=:log10)"
   ]
