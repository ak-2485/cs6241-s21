   "source": [
    "loocv = zeros(length(λs))\n",
    "UTb = UA'*b\n",
    "h = zeros(npts)\n",
    "for k = 1:length(λs)\n",
    "    λ = λs[k]\n",
    "    HS = SA.^2 ./ (SA.^2 .+ λ^2)\n",
    "    r = b - UA*(HS.*UTb)\n",
    "    h = [sum(UA[i,:].^2 .* HS) for i in 1:npts]\n",
    "    loocv[k] = sum(( r ./ (1.0.-h) ).^2)/npts\n",
    "end\n",
    "scatter(λs, loocv, xscale=:log10, yscale=:log10)"
   ]
