   "source": [
    "function legendre(xx, dmax)\n",
    "    P = zeros(length(xx), dmax+1)\n",
    "    P[:,1] .= 1.0\n",
    "    if dmax > 0\n",
    "        P[:,2] .= xx\n",
    "    end\n",
    "    for n = 1:dmax-1\n",
    "        P[:,n+2] .= ( (2*n+1) .* xx .* P[:,n+1] - n * P[:,n] )/(n+1)\n",
    "    end\n",
    "    return P\n",
    "end\n",
    "\n",
    "function nlegendre(xx, dmax)\n",
    "    Q = legendre(xx, dmax)\n",
    "    for n = 0:dmax\n",
    "        Q[:,n+1] .*= sqrt(n+0.5)\n",
    "    end\n",
    "    return Q\n",
    "end"
   ]
